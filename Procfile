web: java $JAVA_OPTS -jar target/*.jar --server.port=$PORT --metrics.jmx.enabled=false --spring.datasource.jmx-enabled=false --spring.jmx.enabled=false --management.security.enabled=false --endpoints.jmx.enabled=false --spring.jpa.database=POSTGRESQL --spring.datasource.platform=postgres --spring.datasource.url=$JDBC_URL --spring.jpa.show-sql=true --spring.jpa.generate-ddl=true --spring.jpa.hibernate.ddl-auto=create-drop --spring.jpa.properties.hibernate.jdbc.lob.non_contextual_creation=true
